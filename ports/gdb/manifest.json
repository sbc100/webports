{
  "name": "GDB",
  "description": "GDB running using NativeClient",
  "version": "0.0.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
  },
  "permissions": [
    "storage",
    {"socket": [
      "resolve-host",
      "tcp-connect"
    ]}
  ]
}
